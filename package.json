{
	"name": "xamarin",
	"displayName": "Xamarin",
	"version": "0.1.0",
	"publisher": "ms-vscode",
	"description": "Visual Studio Code debugger extension to enable Xamarin ",
	"icon": "images/mono-debug-icon.png",
	"categories": [
		"Debuggers"
	],
	"author": {
		"name": "Microsoft Corporation"
	},
	"license": "MIT",
	"private": true,
	"extensionDependencies": [
		"ms-dotnettools.csharp",
		"maxbrister.vscode-nuget-package-manager-improved"
	],
	"scripts": {
		"prepublish": "pwsh ./build.ps1 build",
		"vscode:prepublish": "pwsh ./build.ps1 build",
		"compile": "pwsh ./build.ps1 build",
		"watch": "tsc -w -p ./src/typescript",
		"test": "make tests; mocha --timeout 10000 -u tdd ./out/tests"
	},
	"engines": {
		"vscode": "^1.47.0",
		"node": "^6.3.0"
	},
	"dependencies": {
		"@types/execa": "^2.0.0",
		"@types/lodash": "^4.14.158",
		"@types/vscode": "^1.47.0",
		"@types/xml2js": "^0.4.5",
		"execa": "^4.0.3",
		"lodash": "^4.17.19",
		"rxjs": "^6.6.0",
		"vscode-debugprotocol": "^1.20.0",
		"vscode-nls": "^2.0.2",
		"vscode-test": "^1.4.0",
		"xml2js": "^0.4.23"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joshspicer/vscode-mono-debug.git"
	},
	"bugs": {
		"url": "https://github.com/joshspicer/vscode-mono-debug/issues"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"@types/node": "^6.0.39",
		"mocha": "^2.4.5",
		"typescript": "^3.7.5",
		"vsce": "^1.20.0",
		"vscode-debugadapter-testsupport": "^1.20.0",
		"vscode-nls-dev": "^2.0.1",
		"webpack": "^4.44.0",
		"webpack-cli": "^3.3.12"
	},
	"main": "./out/extension",
	"activationEvents": [
		"onCommand:extension.xamarin-debug.configureExceptions",
		"onCommand:extension.xamarin-debug.startSession",
		"workspaceContains:*.csproj",
		"workspaceContains:**/*.csproj"
	],
	"contributes": {
		"views": {
			"xamarin": [
				{
					"id": "xamarinEmulator",
					"name": "Available Emulators"
				},
				{
					"id": "xamarinNewProject",
					"name": "File > New"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "xamarin",
					"title": "Xamarin",
					"icon": "images/xam-logo.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "xamarinNewProject",
				"contents": "Start a new project. \n [New App](command:xamarinNewProject.newProject)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "xamarinEmulator.refresh",
					"when": "view == xamarinEmulator",
					"group": "navigation"
				}
			],
			"view/item/context": []
		},
		"configuration": {
			"type": "object",
			"title": "%configuration.title%",
			"properties": {
				"xamarin-debug.exceptionOptions": {
					"type": "object",
					"additionalProperties": {
						"type": "string",
						"enum": [
							"never",
							"always",
							"unhandled"
						],
						"enumDescriptions": [
							"%breakMode.never%",
							"%breakMode.always%",
							"%breakMode.unhandled%"
						],
						"description": "%configuration.exceptionOptions.description2%",
						"default": "never"
					},
					"description": "%configuration.exceptionOptions.description%",
					"default": {
						"System.Exception": "never",
						"System.SystemException": "never",
						"System.ArithmeticException": "never",
						"System.ArrayTypeMismatchException": "never",
						"System.DivideByZeroException": "never",
						"System.IndexOutOfRangeException": "never",
						"System.InvalidCastException": "never",
						"System.NullReferenceException": "never",
						"System.OutOfMemoryException": "never",
						"System.OverflowException": "never",
						"System.StackOverflowException": "never",
						"System.TypeInitializationException": "never"
					}
				}
			}
		},
		"languages": [
			{
				"id": "XAML",
				"aliases": [
					"XAML",
					"xaml"
				],
				"extensions": [
					".xaml"
				]
			}
		],
		"grammars": [
			{
				"language": "XAML",
				"scopeName": "source.XAML",
				"path": "./syntaxes/XAML.tmLanguage.json"
			}
		],
		"commands": [
			{
				"command": "extension.xamarin-debug.configureExceptions",
				"title": "%configure.exceptions.command%",
				"category": "Debug"
			},
			{
				"command": "xamarinEmulator.refresh",
				"title": "Refresh"
			},
			{
				"command": "xamarinEmulator.select",
				"title": "Xamarin: Select Emulator"
			},
			{
				"command": "xamarinNewProject.newProject",
				"title": "Xamarin: New Project"
			}
		],
		"breakpoints": [
			{
				"language": "csharp"
			},
			{
				"language": "fsharp"
			}
		],
		"debuggers": [
			{
				"type": "xamarin",
				"label": "Xamarin (Mono C#)",
				"program": "./src/xamarin-debug/bin/Debug/net472/xamarin-debug.exe",
				"osx": {
					"runtime": "mono"
				},
				"linux": {
					"runtime": "mono"
				},
				"aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
				"startSessionCommand": "extension.xamarin-debug.startSession",
				"initialConfigurations": [
					{
						"name": "%mono.launch.config.name%",
						"type": "mono",
						"request": "launch",
						"program": "${workspaceRoot}/program.exe",
						"cwd": "${workspaceRoot}"
					},
					{
						"name": "%mono.attach.config.name%",
						"type": "mono",
						"request": "attach",
						"address": "localhost",
						"port": 55555
					}
				],
				"configurationAttributes": {
					"launch": {
						"required": [
							"program"
						],
						"properties": {
							"program": {
								"type": "string",
								"description": "%mono.launch.program.description%"
							},
							"args": {
								"type": "array",
								"description": "%mono.launch.args.description%",
								"items": {
									"type": "string"
								},
								"default": []
							},
							"cwd": {
								"type": "string",
								"description": "%mono.launch.cwd.description%",
								"default": "."
							},
							"runtimeExecutable": {
								"type": [
									"string",
									"null"
								],
								"description": "%mono.launch.runtimeExecutable.description%",
								"default": null
							},
							"runtimeArgs": {
								"type": "array",
								"description": "%mono.launch.runtimeArgs.description%",
								"items": {
									"type": "string"
								},
								"default": []
							},
							"env": {
								"type": "object",
								"description": "%mono.launch.env.description%",
								"default": {}
							},
							"externalConsole": {
								"type": "boolean",
								"deprecationMessage": "%mono.launch.externalConsole.deprecationMessage%",
								"default": true
							},
							"console": {
								"type": "string",
								"enum": [
									"internalConsole",
									"integratedTerminal",
									"externalTerminal"
								],
								"enumDescriptions": [
									"%mono.launch.console.internalConsole.description%",
									"%mono.launch.console.integratedTerminal.description%",
									"%mono.launch.console.externalTerminal.description%"
								],
								"description": "%mono.launch.console.description%",
								"default": "internalConsole"
							}
						}
					},
					"attach": {
						"required": [
							"port"
						],
						"properties": {
							"port": {
								"type": "number",
								"description": "%mono.attach.port.description%",
								"default": 55555
							},
							"address": {
								"type": "string",
								"description": "%mono.attach.address.description%",
								"default": "undefined"
							}
						}
					}
				}
			}
		]
	}
}
